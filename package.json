  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "clarifai": "^2.9.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "knex": "^0.17.5"
  },
  "devDependencies": {
    "nodemon": "^1.18.11"
  }
}
